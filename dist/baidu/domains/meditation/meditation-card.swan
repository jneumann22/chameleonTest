<view class="screen-container  cml-base cml-view">
  <view class="card-container  cml-base cml-view">
    <view class="card-image  cml-base cml-view">
      <text class=" cml-base cml-text">{{jsonData.item.name}}</text>
    </view>
    <ActionButton text="{{jsonData.item.name}}" defaultButtonStyle="default-orange-button" defaultTextStyle="default-orange-button-text" textColor="text-teal" textSize="font-large" handler="{{pressActionButton}}" class=" cml-view cml-ActionButton" />
    <Footer domain="domain" musePivot="{{jsonData.item.muse_pivot}}" checkBool="{{true}}" checkColorBool="{{true}}" class=" cml-view cml-Footer" />
  </view>
</view>