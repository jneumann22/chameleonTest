.icon-button-container {
  position: absolute;
  right: 30px;
  bottom: 30px;
}
.icon {
  height: 50px;
  width: 50px;
}
.icon-button {
  height: auto;
  width: auto;
  border-radius: 25px;
  opacity: 0.7;
  margin-top: 25px;
}
