<view class="cml-radio " style="{{computedCstyle}}">
  <view class="cml-radio-wrap " bindtap="_cmlEventProxy" data-eventtap="{{['changeSelect']}}">
    <view class="cml-radio-select " style="{{radioStyle}}">
      <view class="{{ radioClass }} " style="{{radioInnerStyle}}">
      </view>
    </view>
    <text class="cml-radio-label ">{{label}}</text>
  </view>
</view>